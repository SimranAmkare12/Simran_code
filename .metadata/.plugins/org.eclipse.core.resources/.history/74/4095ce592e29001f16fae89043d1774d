package Overloading_example;

public class Shapes {

	public void calculate_area(int l,int b) {
		int area = l*b ;
		
	}
	public void calculate_area(int r) {
		double area = 2 *3.14 *r;
		
	}
	public static void main(String[] args) {
		Shapes s = new Shapes();
		s.calculate_area(2);
		s.calculate_area(2, 3);
	}

}
