package Static_example1;

public class Calculator {

	static int num1 ;
	static int num2 ;
	static
	{
		num1 =12;
		num2 =24;
		System.out.println("value are"+num1+num2);
	}
	public static void main(String[] args) {
		
          //  Calculator c= new Calculator();
            System.out.println(" main block");
	}

}
