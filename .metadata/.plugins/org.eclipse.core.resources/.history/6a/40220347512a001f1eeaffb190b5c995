package This_example;

public class Employee {

	 private String name ;
	 
	 public void setname(String name) {
		this.name  = name ;
	 }
	 
	 public String getName() {
		 this.shift();
		 return name;
	 }
	 
	 public void shift() {
		 System.out.println("shift is 9 t0 6 ");
	 }
	 
	  private  Employee(int salary) {
		  System.out.println("Salary is transfer to emp account"+salary);
	  }
	  
	  public Employee(int paisa,String id) {
		  this(paisa);
		  System.out.println("  work has been done "+paisa+id);
	  }
}
