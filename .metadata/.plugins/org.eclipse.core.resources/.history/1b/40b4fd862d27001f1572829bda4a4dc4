package june_10;

public class Demo {

	 public static void main(String args[]) {
		 
		Student s = new Student();
		
		Address ad= new Address();
		
		ad.setCity("Bhopal");
		ad.setDistric(null);
		ad.setPincode("4352");
		ad.setState(" mp ");
		
		s.setS_add(ad);
		
		s.setName(" simran amkare ");
		s.setRolno(1234);
		
		System.out.println(s.getName());
		System.out.println(s.getRolno());
		System.out.println(s.getS_add().getCity());
		System.out.println(s.getS_add().getState());
		System.out.println(s.getS_add().getDistric());
		System.out.println(s.getS_add().getPincode());
		
		Student s1 = new Student( " Amkare  simran",1234,new Address(" Indore"," MP"," ","4576"));
		System.out.println(s1);
		 
	 }
}
